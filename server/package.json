{
  "name": "server",
  "version": "0.1.0",
  "private": true,
  "nodemonConfig": {
    "ignore": [
      "node_modules"
    ],
    "watch": [
      "server"
    ],
    "exec": "tsc --noEmit && ts-node -r tsconfig-paths/register app.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "start": "nodemon"
  },
  "dependencies": {
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.6.1",
    "nodemon": "^2.0.15"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.176",
    "dotenv": "^10.0.0",
    "eslint": "8.0.1",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.2",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "4.4.4"
  }
}
